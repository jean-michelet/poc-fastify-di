{
  "name": "fastify-dependency-inversion-principle",
  "version": "1.0.0",
  "description": "A package to use Pure DI with Fastify",
  "license": "MIT",
  "author": "jean.antoine.michelet@gmail.com",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "test:unit": "borp 'test/**/!(*.integration).test.ts'",
    "test:integration": "npm run db:seed && borp 'test/**/*.integration.test.ts' -c 1",
    "test": "npm run db:seed && borp 'test/**/*.test.ts' -c 1 --reporter=@jsumners/line-reporter --coverage --check-coverage",
    "db:create": "tsx --env-file=.env ./example/scripts/create-database.ts",
    "db:drop": "tsx --env-file=.env ./example/scripts/drop-database.ts",
    "db:migrate": "tsx --env-file=.env ./example/scripts/migrate.ts",
    "db:seed": "tsx --env-file=.env ./example/scripts/seed-database.ts"
  },
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/env": "^5.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.3",
    "@fastify/session": "^11.1.0",
    "@fastify/static": "^8.2.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@fastify/under-pressure": "^9.0.3",
    "@sinclair/typebox": "^0.34.35",
    "@types/node": "^22.15.29",
    "close-with-grace": "^2.2.0",
    "fastify": "^5.3.3",
    "fastify-plugin": "^5.0.1",
    "mysql2": "^3.14.1",
    "postgrator": "^8.0.0",
    "rfdc": "^1.1.3"
  },
  "devDependencies": {
    "@fastify/type-provider-typebox": "^5.1.0",
    "@jsumners/line-reporter": "^1.0.1",
    "@types/rfdc": "^1.1.0",
    "borp": "^0.20.0",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7"
  }
}
